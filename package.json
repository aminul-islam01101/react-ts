{
    "name": "vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx",
        "prettier:check": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|ts|tsx|json)\"",
        "lint:fix": "eslint . --fix",
        "prettier:fix": "prettier --write \"src/**/*.+(js|ts|tsx|json)\"",
        "lint-prettier-fix": "yarn lint:fix && yarn prettier:fix  && yarn lint",
        "lint-prettier-check": "yarn lint:check && yarn prettier:check",
        "lint-prettier": "yarn lint-prettier-fix && yarn lint-prettier-check"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx,js}": "yarn lint-prettier"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "@tailwindcss/forms": "^0.5.3",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "axios": "^1.1.3",
        "csstype": "^3.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.39.1",
        "react-hot-toast": "^2.4.0",
        "react-icons": "^4.10.1",
        "react-redux": "^8.1.1",
        "react-router-dom": "^6.6.1",
        "react-toastify": "^9.0.8",
        "sweetalert2": "^11.6.2",
        "vite-plugin-eslint": "^1.8.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@typescript-eslint/eslint-plugin": "^5.61.0",
        "@typescript-eslint/parser": "^5.61.0",
        "@vitejs/plugin-react": "^4.0.1",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.44.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.1",
        "husky": "^8.0.3",
        "postcss": "^8.4.25",
        "prettier": "^3.0.0",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.0.2",
        "vite": "^4.4.0",
        "lint-staged": "^13.2.2"
    }
}
